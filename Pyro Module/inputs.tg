# simple inputs files for the unsplit CTU hydro scheme

[driver]
max_steps = 2000
tmax = 3.0
cfl = 0.8


[incompressible]


[io]
basename = tg_128_
n_out = 1


[mesh]
nx = 128
ny = 128
xmax = 1.0
ymax = 1.0

xlboundary = periodic
xrboundary = periodic

ylboundary = periodic
yrboundary = periodic


[tg]
rho_s = 42.0
